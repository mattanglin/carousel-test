{
  "author": "Matt Anglin",
  "name": "@mattanglin/carousel",
  "version": "0.0.0",
  "description": "React Carousel Component for Instrument Skills Test",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/components/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": "https://github.com/mattanglin/carousel-test.git",
  "license": "MIT",
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/react-test-renderer": "^16.8.3",
    "@types/webpack-env": "^1.14.0",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "react": "^16.8.6",
    "react-app-rewired": "^2.1.3",
    "react-docgen-typescript": "^1.12.5",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "react-styleguidist": "^9.1.13",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "!**/index.{ts,tsx}",
      "!src/App.tsx"
    ],
    "coverageDirectory": "public/coverage",
    "coverageReporters": [
      "html",
      "json-summary",
      "text",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  },
  "scripts": {
    "start": "react-app-rewired start",
    "lint": "eslint --ext js,ts,tsx src",
    "test": "CI=true react-app-rewired test",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "build": "yarn build:clean; NODE_ENV=production rollup -c",
    "build:clean": "rm -rf ./dist; mkdir ./dist"
  },
  "peerDependencies": {
    "react": "^16.8.6"
  },
  "dependencies": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
